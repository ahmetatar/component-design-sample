{
  "name": "component-design-sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prepare": "husky install",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:snapshots": "jest",
    "test:ci": "ng test --browsers=Headless --no-watch",
    "test:debug": "ng test --browsers=ChromeHeadlessNoSandbox",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "yarn docs:json && start-storybook -p 6006",
    "api:mock": "node ./mock-api/app",
    "build-storybook": "yarn docs:json && build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes",
    "chromatic:local": "npx chromatic --project-token=b9e7ec7a3b18 --exit-zero-on-changes",
    "lint:ci": "eslint",
    "lint": "eslint --fix && git add .",
    "format": "prettier --write . && git add ."
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@cds/sdk": "1.0.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@ngx-storybook/schematics": "1.0.5",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^12.3.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cdk": "^13.1.2",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@angular/elements": "^13.1.0",
    "@babel/core": "^7.16.0",
    "@compodoc/compodoc": "^1.1.16",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-interactions": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-storyshots": "^6.4.22",
    "@storybook/angular": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/testing-angular": "^0.0.12",
    "@types/bootstrap": "^5.1.8",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
