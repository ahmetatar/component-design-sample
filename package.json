    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.6.1",
    "storybook-dark-mode": "^1.0.8",
    "swagger-ui-express": "^4.3.0",
    "typescript": "~4.4.3"
  }
